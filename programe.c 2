#include <stdio.h>

int main() {
    printf("___________________________________________________________________________\n");
    printf("              |              |              |              |               \n");
    printf("              |              |              |              |               \n");
    printf("                                                                       ____\n");
    printf("                            .     V     .                   __3000    |6-  \n");
    printf("__________            .           A           .             |    |    | -  \n");
    printf("       |         .    _________________________    .        |   -|    |2-  \n");
    printf("      -|                         ---                        |    |    | -  \n");
    printf("       |                        -----                       |   -|    |1-  \n");
    printf(" 060  -|                         ---                        |    |    |    \n");
    printf("       |                  10 ----------- 10                >|004-|    |    \n");
    printf("      -|                         ---                        |    |    | -  \n");
    printf("       |                        -----                       |   -|__  |    \n");
    printf(" 040  -|                         ---                        |____|60| |    \n");
    printf("-   ---|<        =======----------+----------=======    ----____2 40| |--  \n");
    printf("       |                         ---                        |    |20| |    \n");
    printf("       |                        -----                       |   -|    |    \n");
    printf("       |                         ---                        |    |    | -  \n");
    printf("       |                  10 ----------- 10                 |   -|    |    \n");
    printf("       |                         ---                        |    |    |    \n");
    printf("       |                        -----                      >|000-|    |1-  \n");
    printf("       |                  20 ----------- 20                 |    |    | -  \n");
    printf("_______|__           ___________________________            |   -|    |2-  \n");
    printf("                                  2                        _|____|    | -  \n");
    printf("                                                                     _|6-__\n");
    printf("              ____________________|____________________                    \n");
    printf("             |   '   |   '   |   '   |   '   |   '   | |     QNH 1015      \n");
    printf("             |2      23      24      25      26      27|                   \n");
    return 0;
}



#include <stdio.h>

int main() {
    printf("___________________________________________________________________________\n");
    printf("    SPEED     |      G/S     |      LOC     |     CAT3     |    AP1+2      \n");
    printf("              |              |              |     DUAL     |    1 FD 2     \n");
    printf("              |              |              |              |    A/THR      \n");
    printf("                                                                           \n");
    printf("                                                                       ____\n");
    printf("__________                .   ∙   ∇   ∙   .                   3000    |6-  \n");
    printf("      -|▄              ∙          ∆          ∙              |‾‾‾‾|‾   | -  \n");
    printf("       |▄             _________________________             |   -|    |2-  \n");
    printf(" 180  -|▄                        ---                        |    |    | -  \n");
    printf("       |                        -----               o      >|040-|    |1-  \n");
    printf("      -|                         ---                        |    |    |    \n");
    printf("       |                  10 -----|----- 10                 |   -|    |    \n");
    printf(" 160  -|                         -|-                o       |    |    | -  \n");
    printf("       |                        --|--                       |   -|__  |    \n");
    printf("      -|                         -|-                    ▲   |____|40| |    \n");
    printf("-   ---|◄        ══════╦----------+----------╦══════    ----   37 20| |    \n");
    printf(" 140  -|               ╩         -|-         ╩          ▼   |‾‾‾‾|00| |--  \n");
    printf("       |                        --|--                       |   -|‾‾  |   /\n");
    printf("      -|                         -|-                o       |    |    |  / \n");
    printf("       ||                 10 -----|----- 10                >|035-|    | /  \n");
    printf(" 120  -||                        ---                        |    |    |/-  \n");
    printf("       ||                     ==-----==             o       |   -|    | 08 \n");
    printf("      -|▄                 20 ----------- 20                 |    |    |    \n");
    printf("       |▄            ___________________________            |   -|    |1-  \n");
    printf(" 100  -|▄                        2310                       |    |    | -  \n");
    printf("       |▄                                                   |   -|    |2-  \n");
    printf("‾‾‾‾‾‾‾‾‾‾                                                  ‾‾‾‾‾‾‾   | -  \n");
    printf("                                                                      |6-  \n");
    printf("                 o       o       ◄|►       o       o                  ‾‾‾‾‾\n");
    printf("                                                             QNH  1013     \n");
    printf("  LFRB        ____________________|____________________                    \n");
    printf("  109.90     |   '   |   '   |   '  ◊|   '   |   '   | |                   \n");
    printf("  7.4  NM    |2      23      24      25      26      27|                   \n");
    printf("‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾‾\n");
    return 0;
}
